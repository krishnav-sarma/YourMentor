#include <stdio.h>

void grade(int marks) {
    if (marks >= 90) {
        printf("Grade A\n");
    } else if (marks >= 80) {
        printf("Grade B\n");
    } else if (marks >= 70) {
        printf("Grade C\n");
    } else if (marks >= 60) {
        printf("Grade D\n");
    } else {
        printf("Fail\n");
    }
}

int main() {
    int marks;

    printf("Enter the marks: ");
    scanf("%d", &marks);
    
    grade(marks);

    return 0;
}